{
    "name": "sawirricardo/laravel-filepond",
    "description": "A FilePond component for your laravel apps",
    "keywords": [
        "sawirricardo",
        "laravel",
        "laravel-filepond"
    ],
    "homepage": "https://github.com/sawirricardo/laravel-filepond",
    "license": "MIT",
    "authors": [
        {
            "name": "sawirricardo",
            "email": "sawir.ricardo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0 | ^9.0 | ^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^5.0 | ^6.0",
        "nunomaduro/larastan": "^1.0 | ^2.0.1",
        "orchestra/testbench": "^6.0 | ^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Sawirricardo\\LaravelFilepond\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sawirricardo\\Filepond\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sawirricardo\\LaravelFilepond\\FilepondServiceProvider"
            ],
            "aliases": {
                "Filepond": "Sawirricardo\\LaravelFilepond\\Facades\\Filepond"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
